<h1 class="text-center mb-3">{{title}}</h1>
<h4>Payloads</h4>
<div class="accordion" id="accordionExample">
  {{#each payloads}}
    <div class="card">
      <div class="card-header" id={{this._id}}>
        <h2 class="mb-0">
          <button class="btn btn-link btn-block text-left" type="button" data-toggle="collapse" data-target="#collapse{{this._id}}" aria-expanded="true" aria-controls={{this._id}}>
            {{this._id}}
          </button>
        </h2>
      </div>
      <div id="collapse{{this._id}}" class="collapse" aria-labelledby={{this._id}} data-parent="#accordionExample">
        <div class="card-body">
          <h4>Headers</h4>
          <p>{{stringify this.headers}}</p>
          <h4>Body</h4>
          <p>{{stringify this.rawBody}}</p>
        </div>
      </div>
    </div>
  {{/each}}
</div>
